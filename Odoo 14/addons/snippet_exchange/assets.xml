            <link type="text/css" rel="stylesheet" href="/snippet_exchange/static/src/scss/style.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/snippet_exchange/static/src/js/snippet.js"/>
